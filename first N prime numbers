import.java.util.Scanner;
 class Main
{
	public static void main(String[] args) {
		static boolean isPrime(int n){
		    if(n<2)return false;
		    for(int i=2;i<=n/2;i++)
		    if(n%i==0)return false;
		    return true;
		}
		    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();

        int count = 0, num = 2;
        while (count < N) {
            if (isPrime(num)) {
                System.out.print(num + " ");
                count++;
            }
            num++;
        }

	}
}
